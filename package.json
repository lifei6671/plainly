{
  "name": "plainly",
  "author": "mdnice",
  "description": "a markdown editor with the function of style edition",
  "version": "1.7.3",
  "private": false,
  "main": "lib/Lib.js",
  "module": "lib/Lib.js",
  "homepage": "https://mdnice.disign.me",
  "license": "GPL-3.0",
  "typings": "./lib/index.d.ts",
  "repository": {
    "type": "git",
    "url": "https://github.com/mdnice/plainly"
  },
  "scripts": {
    "start": "vite",
    "watch": "node ./watch.js",
    "build": "vite build",
    "test": "node scripts/test.js",
    "analyze": "source-map-explorer docs/assets/*.js",
    "lint": "eslint src --ext ts,tsx,js,jsx --fix",
    "publish:npm": "cross-env NODE_ENV=production && rm -rf lib && mkdir lib && cross-env BABEL_ENV=production npx babel src --out-dir lib --copy-files --extensions \".js,.jsx\"",
    "storybook": "npm run publish:npm && start-storybook -p 9001 -c .storybook",
    "dev": "vite",
    "preview": "vite preview",
    "tauri": "tauri",
    "tauri:dev": "tauri dev",
    "tauri:build": "tauri build",
    "postinstall": "node scripts/copy-jieba-wasm.js"
  },
  "dependencies": {
    "@ant-design/icons": "^6.1.0",
    "@aws-sdk/client-s3": "^3.689.0",
    "@jsquash/webp": "^1.5.0",
    "@sitdown/juejin": "^1.1.3",
    "@sitdown/wechat": "^1.1.4",
    "@sitdown/zhihu": "^1.1.2",
    "@tauri-apps/api": "^1.6.0",
    "@uiw/react-codemirror": "^1.0.28",
    "ali-oss": "^6.1.1",
    "antd": "^4.24.15",
    "axios": "1.12.0",
    "browser-image-compression": "^2.0.2",
    "classnames": "^2.3.2",
    "codemirror": "^5.65.16",
    "diff-match-patch": "^1.0.4",
    "enquire.js": "^2.1.6",
    "highlight.js": "^11.9.0",
    "idb": "^8.0.3",
    "jieba-wasm": "^2.4.0",
    "juice": "^5.2.0",
    "lodash.debounce": "^4.0.8",
    "lodash.throttle": "^4.1.1",
    "lunr": "^2.3.9",
    "markdown-it": "12.3.2",
    "markdown-it-deflist": "^2.0.3",
    "markdown-it-footnote": "^3.0.1",
    "markdown-it-implicit-figures": "^0.9.0",
    "markdown-it-katex": "^2.0.3",
    "markdown-it-ruby": "^0.1.1",
    "markdown-it-table-of-contents": "^0.4.4",
    "mathjax": "^3.0.1",
    "mermaid": "^10.9.1",
    "mobx": "^5.9.0",
    "mobx-react": "^5.4.3",
    "mutationobserver-shim": "^0.3.7",
    "prettier": "^1.19.1",
    "prop-types": "^15.8.1",
    "qiniu-js": "^2.5.4",
    "react": "18.2.0",
    "react-dom": "18.2.0",
    "react-helmet": "^5.2.1",
    "remark-parse": "^11.0.0",
    "sitdown": "^1.1.1",
    "strip-markdown": "^6.0.0",
    "unified": "^11.0.5"
  },
  "devDependencies": {
    "@babel/cli": "^7.24.1",
    "@babel/core": "^7.24.1",
    "@babel/eslint-parser": "^7.24.1",
    "@babel/plugin-proposal-decorators": "^7.24.1",
    "@babel/plugin-transform-class-properties": "^7.24.1",
    "@babel/preset-react": "^7.24.1",
    "@storybook/react": "^4.1.11",
    "@tauri-apps/cli": "^1.6.0",
    "@types/node": "^25.1.0",
    "@types/react": "^19.2.10",
    "@types/react-dom": "^19.2.3",
    "@vitejs/plugin-react": "^4.2.1",
    "babel-core": "7.0.0-bridge.0",
    "babel-jest": "23.6.0",
    "babel-plugin-inline-import": "^3.0.0",
    "chalk": "^2.4.2",
    "chokidar": "^3.2.1",
    "cross-env": "^6.0.3",
    "dotenv": "6.0.0",
    "dotenv-expand": "4.2.0",
    "eslint": "^8.57.0",
    "eslint-config-airbnb": "^19.0.4",
    "eslint-config-prettier": "^9.1.0",
    "eslint-plugin-babel": "^5.3.0",
    "eslint-plugin-flowtype": "2.50.1",
    "eslint-plugin-import": "^2.29.1",
    "eslint-plugin-jsx-a11y": "^6.8.0",
    "eslint-plugin-prettier": "^4.2.1",
    "eslint-plugin-react": "^7.34.1",
    "eslint-plugin-react-hooks": "^4.6.0",
    "fs-extra": "7.0.1",
    "husky": "^3.0.7",
    "identity-obj-proxy": "3.0.0",
    "jest": "23.6.0",
    "jest-pnp-resolver": "1.0.2",
    "jest-resolve": "23.6.0",
    "jest-watch-typeahead": "^0.2.1",
    "lint-staged": "^9.4.0",
    "pretty-quick": "^1.11.1",
    "react-app-polyfill": "^0.2.1",
    "shelljs": "^0.8.3",
    "source-map-explorer": "^2.0.1",
    "typescript": "^5.9.3",
    "vite": "^5.2.0"
  },
  "build": {
    "productName": "Markdown Nice",
    "appId": "com.mdnice.www",
    "mac": {
      "target": [
        "dmg",
        "zip"
      ]
    },
    "win": {
      "target": [
        "nsis",
        "zip"
      ]
    },
    "files": [
      "build",
      "main.js",
      "package.json"
    ]
  },
  "browserslist": [
    ">0.2%",
    "not dead",
    "not ie <= 11",
    "not op_mini all"
  ],
  "jest": {
    "collectCoverageFrom": [
      "src/**/*.{js,jsx,ts,tsx}",
      "!src/**/*.d.ts"
    ],
    "resolver": "jest-pnp-resolver",
    "setupFiles": [
      "react-app-polyfill/jsdom"
    ],
    "testMatch": [
      "<rootDir>/src/**/__tests__/**/*.{js,jsx,ts,tsx}",
      "<rootDir>/src/**/?(*.)(spec|test).{js,jsx,ts,tsx}"
    ],
    "testEnvironment": "jsdom",
    "testURL": "http://localhost",
    "transform": {
      "^.+\\.(js|jsx|ts|tsx)$": "<rootDir>/node_modules/babel-jest",
      "^.+\\.css$": "<rootDir>/config/jest/cssTransform.js",
      "^(?!.*\\.(js|jsx|ts|tsx|css|json)$)": "<rootDir>/config/jest/fileTransform.js"
    },
    "transformIgnorePatterns": [
      "[/\\\\]node_modules[/\\\\].+\\.(js|jsx|ts|tsx)$",
      "^.+\\.module\\.(css|sass|scss)$"
    ],
    "moduleNameMapper": {
      "^react-native$": "react-native-web",
      "^.+\\.module\\.(css|sass|scss)$": "identity-obj-proxy"
    },
    "moduleFileExtensions": [
      "web.js",
      "js",
      "web.ts",
      "ts",
      "web.tsx",
      "tsx",
      "json",
      "web.jsx",
      "jsx",
      "node"
    ],
    "watchPlugins": []
  },
  "babel": {
    "presets": [
      "@babel/preset-react"
    ],
    "plugins": [
      [
        "@babel/plugin-proposal-decorators",
        {
          "legacy": true
        }
      ],
      [
        "@babel/plugin-transform-class-properties",
        {
          "loose": true
        }
      ]
    ],
    "env": {
      "production": {
        "plugins": [
          [
            "babel-plugin-inline-import",
            {
              "extensions": [
                ".md"
              ]
            }
          ]
        ]
      }
    }
  },
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged"
    }
  },
  "lint-staged": {
    "src/**/*.{jsx,txs,ts,js,json}": [
      "prettier --write",
      "eslint --fix",
      "git add"
    ]
  },
  "packageManager": "pnpm@9.12.0"
}
